# Server configuration
PORT=3000

# GitHub configuration
# Generate token at https://github.com/settings/tokens
# Required for private repositories and to avoid rate limits
GITHUB_TOKEN=

# Cache configuration
# Duration in milliseconds (default: 30 minutes)
CACHE_DURATION=1800000

# CORS configuration
# Comma-separated list of allowed origins, use * for all origins
ALLOWED_ORIGINS=*

# Logging configuration
DEBUG=false

# Syntax highlighting
DEFAULT_THEME=github 