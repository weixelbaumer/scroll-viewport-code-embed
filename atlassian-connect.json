{
    "key": "github-code-renderer",
    "name": "Apryse GitHub Code Renderer",
    "description": "Render GitHub code snippets for embedding in Confluence",
    "baseUrl": "https://dev.tandav.com",
    "vendor": {
        "name": "Apryse",
        "url": "https://dev.tandav.com"
    },
    "authentication": {
        "type": "none"
    },
    "apiVersion": 1,
    "modules": {
        "generalPages": [
            {
                "key": "github-code-renderer",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "Apryse GitHub Code Renderer"
                },
                "url": "/index.html"
            }
        ],
        "dynamicContentMacros": [
            {
                "key": "github-code-block",
                "name": {
                    "value": "GitHub Code Block"
                },
                "url": "/macro",
                "description": {
                    "value": "Embed code from GitHub repositories"
                },
                "width": "100%",
                "height": "300px",
                "bodyType": "none"
            }
        ]
    }
} 